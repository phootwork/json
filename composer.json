{
	"name" : "phootwork/json",
	"type" : "library",
	"description" : "PHP json wrapper library",
	"authors" : [{
			"name" : "Thomas Gossmann",
			"homepage" : "http://gos.si"
		}
	],
	"license" : "MIT",
	"keywords" : [
		"json"
	],
	"support" : {
		"issues" : "https://github.com/phootwork/json/issues"
	},
	"autoload" : {
		"psr-4" : {
			"phootwork\\json\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"phootwork\\json\\tests\\" : "tests"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"phootwork/collection" : "^1.5"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^5.7",
		"sami/sami" : "^4|^3"
	},
	"scripts" : {
		"test" : "phpunit",
		"api" : "sami.php update sami.php"
	}
}